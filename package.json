{
  "name": "grafico-ql",
  "title": "grafico-ql",
  "version": "3.0.5",
  "description": "A minimal graphql client.",
  "keywords": [
    "graphql",
    "client",
    "fetch",
    "asynchron",
    "request",
    "graphql-request",
    "graphql-client"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d3mo17/grafico-ql.git"
  },
  "scripts": {
    "document": "node_modules/jsdoc-to-markdown/bin/cli.js --no-cache --separators --private src/*.js > api.md",
    "build": "node scripts/build.js",
    "test": "node_modules/jest/bin/jest.js"
  },
  "directories": {
    "test": "test"
  },
  "main": "./src/GraficoQL.js",
  "browser": "dist/grafico-ql.min.js",
  "devDependencies": {
    "jest-fetch-mock": "^3.0.3",
    "jest": "^28.1.3",
    "jsdoc-to-markdown": "^7.1.1",
    "requirejs": "^2.3.6",
    "uglify-js": "^3.8.1"
  },
  "author": "Daniel Moritz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d3mo17/grafico-ql/issues"
  },
  "homepage": "https://github.com/d3mo17/grafico-ql",
  "runkitExampleFilename": "runkit-example.js",
  "jest": {
    "automock": false,
    "resetMocks": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  }
